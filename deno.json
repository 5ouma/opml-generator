{
  "name": "@5ouma/opml-generator",
  "version": "1.0.2",
  "exports": "./src/main.ts",
  "publish": {
    "include": ["LICENSE", "README.md", "deno.json", "src/"],
    "exclude": ["**/*.test.ts"]
  },
  "lock": { "frozen": true },
  "tasks": {
    "gen": "deno run -RWE='NITTER_DOMAIN' src/main.ts",
    "test": "deno test --doc -RWE='NITTER_DOMAIN' --parallel --shuffle",
    "cov": "deno task test --coverage && deno coverage --lcov > coverage.lcov"
  },
  "imports": {
    "@libs/xml": "jsr:@libs/xml@6.0.0",
    "@std/assert": "jsr:@std/assert@1.0.6",
    "@std/cli": "jsr:@std/cli@1.0.6",
    "@std/path": "jsr:@std/path@1.0.6",
    "@std/toml": "jsr:@std/toml@1.0.1",
    "@wok/case": "jsr:@wok/case@1.0.1"
  }
}
