{
  "name": "@5ouma/opml-generator",
  "version": "1.0.6",
  "exports": "./src/main.ts",
  "publish": {
    "include": ["LICENSE", "README.md", "deno.json", "src/"],
    "exclude": ["**/*.test.ts"]
  },
  "permissions": {
    "default": { "read": true, "write": true, "env": ["NITTER_DOMAIN"] }
  },
  "lock": { "frozen": true },
  "tasks": {
    "gen": "deno run -P src/main.ts",
    "test": "deno test -P --parallel --doc --shuffle",
    "test:cov": "deno task test --coverage --junit-path='junit.xml'"
  },
  "imports": {
    "@libs/xml": "jsr:@libs/xml@7.0.2",
    "@std/assert": "jsr:@std/assert@1.0.14",
    "@std/cli": "jsr:@std/cli@1.0.22",
    "@std/path": "jsr:@std/path@1.1.2",
    "@std/toml": "jsr:@std/toml@1.0.10",
    "@wok/case": "jsr:@wok/case@1.0.2"
  }
}
